{
  "expo": {
    "name": "IoT Apps",
    "slug": "ssmsiotapps",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ssmsiotapps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "id.co.ssms.ssmsiotapps",
      "infoPlist": {
        "UISupportsDocumentBrowser": true,
        "UIFileSharingEnabled": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fdfdfe",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "id.co.ssms.ssmsiotapps"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#0D8AF1",
          "dark": {
            "backgroundColor": "#0D8AF1"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "977531a6-09ca-46ab-9760-a0cb0e264f77"
      }
    },
    "owner": "iot-ssms",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/977531a6-09ca-46ab-9760-a0cb0e264f77"
    }
  }
}
